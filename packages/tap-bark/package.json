{
    "name": "tap-bark",
    "version": "2.2.0",
    "description": "TAP parser for Alsatian",
    "main": "dist/tap-bark.js",
    "typings": "dist/tap-bark.d.ts",
    "bin": {
        "tap-bark": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "lint": "tslint \"./**/*.ts\" --exclude \"./**/*.d.ts\" --exclude \"./**/*.test.ts\" --exclude \"./node_modules/**/*.ts\"",
        "lint:fix": "npm run lint -- --fix",
        "prepublishOnly": "npm test && npm run build",
        "test": "install-self && npm run lint && npm run test:coverage",
        "test:coverage": "nyc npm run test:unit -- --hide-progress && nyc report",
        "test:unit": "alsatian \"./**/*.test.ts\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alsatian-test/alsatian/tree/master/tap-bark"
    },
    "author": "James Monger <jameskmonger@hotmail.co.uk> (http://jamesmonger.com)",
    "contributors": [
        "James Richford"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alsatian-test/alsatian/issues"
    },
    "homepage": "https://github.com/alsatian-test/alsatian/tree/master/packages/tap-bark#readme",
    "dependencies": {
        "@types/ink": "^0.5.2",
        "@types/node": ">=0.0.2",
        "duplexer": "^0.1.1",
        "ink": "^0.5.1",
        "tap-parser": "^5.0.0",
        "through2": "^3.0.1"
    },
    "devDependencies": {
        "@types/through2": "^2.0.34",
        "alsatian": "^3.1.0",
        "chalk": "^2.4.1",
        "install-self": "^1.0.0-rc.1",
        "nyc": "^10.0.0",
        "prettier": "^1.10.2",
        "tslint": "^5.11.0",
        "tslint-plugin-prettier": "^1.3.0",
        "typescript": "^3.5.2"
    },
    "nyc": {
        "all": true,
        "check-coverage": true,
        "branches": 100,
        "lines": 100,
        "functions": 100,
        "statements": 100,
        "extension": [
            ".ts",
            ".tsx"
        ],
        "include": [
            "**/*.ts",
            "**/*.tsx"
        ],
        "exclude": [
            "test",
            "dist"
        ],
        "reporter": [
            "lcov"
        ]
    },
    "gitHead": "c9378ae3f68dc87ec90481659d86364020411b1a"
}
