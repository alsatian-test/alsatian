{
	"name": "alsatian-vscode",
	"displayName": "alsatian",
	"description": "VSCode extension for the Alsatian test framework",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run build",
		"build": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "npm run test:lint && npm run test:unit",
		"test:lint": "eslint src --ext ts --max-warnings 0",
		"test:unit": "alsatian \"./src/**/*.spec.ts\""
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.5",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/node": "^12.11.7",
		"@types/react": "^16.9.23",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"alsatian-enzyme": "^1.0.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"alsatian": "^3.2.1",
		"find-nearest-file": "^1.1.0",
		"ts-node": "^8.6.2"
	}
}
