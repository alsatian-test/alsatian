{
  "name": "alsatian-packages",
  "private": true,
  "version": "1.0.0",
  "description": "repository for all alsatian packages",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap && npm run build:tap-bark && lerna link",
    "build": "lerna run build",
    "build:tap-bark": "npm run build:code --prefix packages/tap-bark",
    "clean": "lerna run clean",
    "lint:fix": "lerna run lint:fix",
    "test": "lerna run test",
    "test:changes": "lerna run test --since master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alsatian-test/alsatian.git"
  },
  "author": "James Adarich <=> (=)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alsatian-test/alsatian/issues"
  },
  "homepage": "https://github.com/alsatian-test/alsatian#readme",
  "devDependencies": {
    "lerna": "^3.4.3"
  }
}
